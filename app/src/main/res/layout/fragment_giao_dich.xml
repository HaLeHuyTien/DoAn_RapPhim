<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cardview_light_background"
    tools:context=".packageDangKyDangNhap.packageDangNhap.packageThongTinUser.GiaoDichFragment">

    <TextView
        android:id="@+id/txtHoVaTenTT"
        android:layout_width="190dp"
        android:layout_height="42dp"
        android:layout_marginTop="100dp"
        android:gravity="center|center_vertical"
        android:text="Họ Và Tên"
        android:textColor="#F44336"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="112dp"
        android:layout_height="49dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="36dp"
        android:text="Danh sách các đơn đặt lịch"
        android:textColor="@color/cardview_dark_background"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <ListView
        android:id="@+id/listDonDat"
        android:layout_width="0dp"
        android:layout_height="164dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/txtTTThang" />

    <TextView
        android:id="@+id/txtTTNam"
        android:layout_width="0dp"
        android:layout_height="40sp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="16dp"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/txtHoVaTenTT" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="110dp"
        android:layout_height="41dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="36dp"
        android:text="Tổng tiền mua vé theo tháng"
        android:textColor="@color/cardview_dark_background"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/txtTTThang"
        android:layout_width="0dp"
        android:layout_height="40sp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="16dp"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/spinnerThang"
        app:layout_constraintTop_toBottomOf="@+id/txtTTNam" />

    <Spinner
        android:id="@+id/spinnerThang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@+id/txtTTThang"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/txtTTNam" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="111dp"
        android:layout_height="45dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="63dp"
        android:text="Tổng tiền mua vé theo năm"
        android:textColor="@color/cardview_dark_background"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtHoVaTenTT" />

</androidx.constraintlayout.widget.ConstraintLayout>